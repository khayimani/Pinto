generator client {
provider = "prisma-client-js"
}


datasource db {
provider = "postgresql"
url = env("DATABASE_URL")
}


model Account {
id String @id @default(cuid())
provider String
accessToken String
refreshToken String?
meta Json?
createdAt DateTime @default(now())
}


model Connector {
id String @id @default(cuid())
provider String
category String
action String
version String
manifest Json
createdAt DateTime @default(now())
}